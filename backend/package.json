{
  "name": "medusa-starter-default",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "dev": "node ./develop.mjs",
    "dev:inspect": "node --inspect ./develop.mjs",
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "node ./build.mjs",
    "migration:init:account": "MIKRO_ORM_CLI=./src/modules/account/mikro-orm.config.dev.ts mikro-orm migration:create --initial -n InitialSetupMigration",
    "migration:init:restaurant": "MIKRO_ORM_CLI=./src/modules/restaurant/mikro-orm.config.dev.ts mikro-orm migration:create --initial -n InitialSetupMigration",
    "migration:init:delivery": "MIKRO_ORM_CLI=./src/modules/delivery/mikro-orm.config.dev.ts mikro-orm migration:create --initial -n InitialSetupMigration",
    "migration:create:account": " MIKRO_ORM_CLI=./src/modules/account/mikro-orm.config.dev.ts mikro-orm migration:create",
    "migration:create:restaurant": " MIKRO_ORM_CLI=./src/modules/restaurant/mikro-orm.config.dev.ts mikro-orm migration:create",
    "migration:create:delivery": " MIKRO_ORM_CLI=./src/modules/delivery/mikro-orm.config.dev.ts mikro-orm migration:create",
    "test:integration": "cross-env TEST_TYPE=integration jest --runInBand --forceExit",
    "test:integration:modules": "cross-env TEST_TYPE=integration:modules jest --runInBand --forceExit",
    "product:seed": "medusa-product-seed ./seed-data.js"
  },
  "dependencies": {
    "@medusajs/admin": "preview",
    "@medusajs/api-key": "preview",
    "@medusajs/auth": "preview",
    "@medusajs/cache-inmemory": "preview",
    "@medusajs/cache-redis": "preview",
    "@medusajs/cart": "preview",
    "@medusajs/currency": "preview",
    "@medusajs/customer": "preview",
    "@medusajs/event-bus-local": "preview",
    "@medusajs/event-bus-redis": "preview",
    "@medusajs/fulfillment": "preview",
    "@medusajs/fulfillment-manual": "preview",
    "@medusajs/inventory-next": "preview",
    "@medusajs/medusa": "preview",
    "@medusajs/modules-sdk": "preview",
    "@medusajs/orchestration": "preview",
    "@medusajs/order": "preview",
    "@medusajs/payment": "preview",
    "@medusajs/pricing": "preview",
    "@medusajs/product": "preview",
    "@medusajs/promotion": "preview",
    "@medusajs/region": "preview",
    "@medusajs/sales-channel": "preview",
    "@medusajs/stock-location-next": "preview",
    "@medusajs/store": "preview",
    "@medusajs/tax": "preview",
    "@medusajs/types": "preview",
    "@medusajs/user": "preview",
    "@medusajs/utils": "preview",
    "@medusajs/workflow-engine-redis": "preview",
    "@medusajs/workflows-sdk": "preview",
    "@tanstack/react-query": "4.22.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.17.2",
    "jsonwebtoken": "^9.0.2",
    "medusa-interfaces": "*",
    "prism-react-renderer": "^2.0.4",
    "typeorm": "^0.3.16",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@medusajs/medusa-cli": "^1.3.21",
