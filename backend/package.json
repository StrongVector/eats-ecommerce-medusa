    "@mikro-orm/core": "5.9.7",
    "@mikro-orm/migrations": "5.9.7",
    "@mikro-orm/postgresql": "5.9.7",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.11",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "babel-jest": "^29.7.0",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "get-port": "^5.1.1",
    "jest": "^29.7.0",
    "medusa-test-utils": "1.1.44-snapshot-20240403120107",
    "prettier": "^3.2.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^4.5.2"
  },
  "overrides": {
